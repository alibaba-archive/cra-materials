{
  "name": "@icedesign/cra-scaffold",
  "version": "0.1.3",
  "description": "Create react app scaffold for iceworks",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "src/",
    "build/",
    "public/",
    "tests/",
    "_gitignore",
    "_editorconfig",
    "_eslintignore",
    "_eslintrc",
    "jsconfig.json",
    "screenshot.png"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint --cache --ext .js,.jsx src",
    "test": "react-scripts test",
    "prepublishOnly": "PUBLIC_URL=./ npm run build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "node-sass": "^4.12.0",
    "react-scripts": ">=3.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "iceworks": {
    "type": "react",
    "adapter": "adapter-cra-v1"
  },
  "scaffoldConfig": {
    "builder": "cra",
    "name": "create-react-app",
    "title": "create-react-app scaffold",
    "screenshot": "//img.alicdn.com/tfs/TB1hvbYbQL0gK0jSZFtXXXQCXXa-2866-1522.png"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ice-lab/cra-materials/tree/master/scaffolds/Lite"
  }
}
